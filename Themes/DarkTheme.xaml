<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Background" Color="#CCCCCC"></SolidColorBrush>
    <SolidColorBrush x:Key="Text" Color="#333333"></SolidColorBrush>
    <SolidColorBrush x:Key="Button" Color="#007ACC"></SolidColorBrush>
    <SolidColorBrush x:Key="LightText" Color="#FFFFFF"></SolidColorBrush>
    <SolidColorBrush x:Key="DarkText" Color="#000000"></SolidColorBrush>
    <SolidColorBrush x:Key="ButtonOverMouse" Color="#005C91"></SolidColorBrush>
    <Color x:Key="DropShadow">#999999</Color>
</ResourceDictionary>
